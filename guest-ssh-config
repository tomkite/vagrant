host github.com
  HostName %h
  user git
  IdentityFile /vagrant/.ssh/tomkite_rsa

Host master
   HostName %h
   User vagrant
   #Port <PORT>
   UserKnownHostsFile /dev/null
   StrictHostKeyChecking no
   PasswordAuthentication no
   IdentityFile /vagrant/.ssh/tomkite_rsa
   IdentitiesOnly yes
   LogLevel FATAL

Host slave1
   HostName %h
   User vagrant
   #Port <PORT>
   UserKnownHostsFile /dev/null
   StrictHostKeyChecking no
   PasswordAuthentication no
   IdentityFile /vagrant/.ssh/tomkite_rsa
   IdentitiesOnly yes
   LogLevel FATAL

Host slave2
   HostName %h
   User vagrant
   #Port <PORT>
   UserKnownHostsFile /dev/null
   StrictHostKeyChecking no
   PasswordAuthentication no
   IdentityFile /vagrant/.ssh/tomkite_rsa
   IdentitiesOnly yes
   LogLevel FATAL
