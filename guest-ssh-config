Host master
   HostName %h
   User vagrant
   Port 22
   UserKnownHostsFile /dev/null
   StrictHostKeyChecking no
   PasswordAuthentication no
   IdentityFile /vagrant/.vagrant/machines/master/virtualbox/private_key
   IdentitiesOnly yes
   LogLevel FATAL

